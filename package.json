{
  "name": "streamist",
  "version": "1.0.0",
  "description": "Multiple Twitch stream viewer built with Vue",
  "main": "index.html",
  "scripts": {
    "build": "webpack --mode production",
    "watch": "webpack-dev-server --open --mode development",
    "deploy": "webpack --mode production && gh-pages -d dist",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "update-snapshots": "jest --updateSnapshot",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielHearn/streamist.git"
  },
  "author": "Daniel Hearn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DanielHearn/streamist/issues"
  },
  "browserslist": [
    "> 0.25%",
    "maintained node versions",
    "not dead"
  ],
  "homepage": "https://github.com/DanielHearn/streamist#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addon-storyshots": "^5.1.9",
    "@storybook/addon-storyshots-puppeteer": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/vue": "^5.1.9",
    "@vue/test-utils": "^1.0.0-beta.29",
    "app-manifest-webpack-plugin": "^1.2.0",
    "autoprefixer": "^9.6.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-vue": "^2.0.2",
    "browser-sync": "^2.26.7",
    "browser-sync-client": "^2.26.6",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "date-fns": "^1.30.1",
    "del": "^3.0.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-vue": "^5.2.3",
    "file-loader": "^3.0.1",
    "gh-pages": "^1.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-loader": "0.0.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "jest-vue-preprocessor": "^1.5.0",
    "lodash": "^4.17.19",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "pug-html-loader": "^1.1.5",
    "run-sequence": "^2.2.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "vue": "^2.6.10",
    "vue-jest": "3.0.4",
    "vue-loader": "^15.7.1",
    "vue-template-compiler": "^2.6.10",
    "vuedraggable": "^2.23.0",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "base64url": "^2.0.0",
    "boom": "^7.3.0",
    "hoek": "^5.0.4",
    "vue-style-loader": "^4.1.2",
    "vuex": "^3.1.1",
    "yup": "^0.27.0"
  },
  "gh-pages-deploy": {
    "staticpath": "dist",
    "commit": "Deploy to gh-pages",
    "noprompt": false
  },
  "jest": {
    "setupFiles": [
      "./setup-jest.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "vue-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(@storybook/.*\\.vue$))"
    ],
    "cacheDirectory": "./node_modules/.cache/jest"
  }
}
